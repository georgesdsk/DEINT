             x:Class="ejercicio1.Views.AboutPage"
             xmlns:vm="clr-namespace:ejercicio1.ViewModels"
             Title="{Binding Title}">
    
    <ContentPage.BindingContext>
        <vm:AboutViewModel />
    </ContentPage.BindingContext>
    
    <ContentPage.Resources>
        <ResourceDictionary>
            <Color x:Key="Accent">#96d1ff</Color>
        </ResourceDictionary>
    </ContentPage.Resources>

    <StackLayout>
        <Label Text="Tu nombre por coñoo:"></Label>
        <Entry x:Name="entry"></Entry>
        <Label Text="Tu apellido por coñoo:"></Label>
        <Entry x:Name="entry"></Entry>
        <Label Text="Tu nombre por coñoo:"></Label>
